{
	"name": "smfRecordDataFlow",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"linkedService": {
						"referenceName": "Snowflake_UnityCloudFFA_MobilityCoreLocal",
						"type": "LinkedServiceReference"
					},
					"name": "streamOfSMFRecords"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "SinkADXMobilityCore_SMF_FEDERATE_PROD",
						"type": "DatasetReference"
					},
					"name": "sinkToADX"
				}
			],
			"transformations": [
				{
					"name": "derivedColumn1"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     format: 'table',",
				"     store: 'snowflake') ~> streamOfSMFRecords",
				"streamOfSMFRecords derive(RECORD_CONTENT = replace('\\\"job\\\": \\\"pfcp\\\",' ,'\\\"job\\\": \\\"pfcp\\\",\\\"appName\\\": \\\"smfcc_interface\\\",')) ~> derivedColumn1",
				"derivedColumn1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     input(",
				"          SF_LOAD_TS_EST as timestamp,",
				"          RECORD_CONTENT as string",
				"     ),",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sinkToADX"
			]
		}
	}
}